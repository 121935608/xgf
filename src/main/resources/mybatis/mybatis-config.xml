<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="cacheEnabled" value="true" /><!-- 全局映射器启用缓存 -->
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="logImpl" value="SLF4J"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.xingrongjinfu.system.user.model.User" alias="User"/>
		<typeAlias type="com.xingrongjinfu.system.role.model.Role" alias="Role"/>
		<typeAlias type="com.xingrongjinfu.system.permission.model.Permission" alias="Permission"/>
		<typeAlias type="com.xingrongjinfu.system.user.model.UserRole" alias="UserRole"/>
		<typeAlias type="com.xingrongjinfu.system.syscode.model.SysCode" alias="SysCode"/>
		<typeAlias type="com.xingrongjinfu.system.operlog.model.OperLog" alias="OperLog"/>
		<typeAlias type="com.xingrongjinfu.system.userlog.model.UserLog" alias="UserLog"/>
		<typeAlias type="com.xingrongjinfu.system.role.model.RolePermission" alias="RolePermission"/>
		<typeAlias type="com.xingrongjinfu.system.merchant.model.Merchant" alias="Merchant"/>
		<typeAlias type="com.xingrongjinfu.system.merchant.model.AccountInfo" alias="AccountInfo"/>
		<typeAlias type="com.xingrongjinfu.system.merchant.model.Balance" alias="Balance"/>
		<typeAlias type="com.xingrongjinfu.commodity.classification.model.Category" alias="Category"/>
		<typeAlias type="com.xingrongjinfu.commodity.label.model.Label" alias="Label"/>
		<typeAlias type="com.xingrongjinfu.system.storeaffairs.model.Store" alias="Store"/>
		<typeAlias type="com.xingrongjinfu.system.storeaffairs.model.BankAccount" alias="BankAccount"/>
		<typeAlias type="com.xingrongjinfu.system.storeaffairs.model.Repay" alias="Repay"/>
		<typeAlias type="com.xingrongjinfu.system.supervisor.model.Supervisor" alias="Supervisor"/>
		<typeAlias type="com.xingrongjinfu.system.storeaffairs.model.Order" alias="Order"/>
		<typeAlias type="com.xingrongjinfu.system.help.model.Help" alias="Help"/>
		<typeAlias type="com.xingrongjinfu.system.document.model.Document" alias="Document"/>
		<typeAlias type="com.xingrongjinfu.system.setting.model.Setting" alias="Setting"/>
		<typeAlias type="com.xingrongjinfu.system.pays.model.Pays" alias="Pays"/>
		<typeAlias type="com.xingrongjinfu.system.commodity.model.Commodity" alias="Commodity"/>
		<typeAlias type="com.xingrongjinfu.system.sale.model.Sale" alias="Sale"/>
		<typeAlias type="com.xingrongjinfu.system.financial.model.Financial" alias="Financial"/>
		<typeAlias type="com.xingrongjinfu.system.order.model.Order" alias="Orders"/>
		<typeAlias type="com.xingrongjinfu.system.order.model.OrderDetail" alias="OrderDetail"/>

		<!-- 分页 -->
		<typeAlias type="org.framework.base.util.TableDataInfo" alias="TableDataInfo"/>
		<typeAlias type="org.framework.base.util.PageUtilEntity" alias="PageUtilEntity"/>
	</typeAliases>

	<plugins>
		<plugin interceptor="org.apache.ibatis.plugin.ExecutorPageMethodInterceptor">
			<property name="dialect" value="mysql" />
			<property name="pageSqlId" value=".*pageInfoQuery.*" />
		</plugin>
	</plugins>
</configuration>
